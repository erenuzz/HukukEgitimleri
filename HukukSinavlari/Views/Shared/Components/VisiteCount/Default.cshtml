@model List<VisitCountModel>

<div class="table-responsive">
    <table class="table table-hover">
        <thead>
            <tr>
                <th>En fazla ziyaretçi gelen şehir</th>
                <th>Toplam Kaç Ziyaretçi</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var cityCount in Model)
            {
                <tr>
                    <td>@cityCount.City</td>
                    <td>@cityCount.Count</td>
                </tr>
            }
        </tbody>
    </table>
</div>